#!/usr/bin/python -u
# The -u makes output unbuffered, so it will show up immediately
# Austin Colcord and Nick Scheuring
import sys
from Bridge4 import Bridge


def main():
    id = sys.argv[1]
    LAN = sys.argv[2:]
    myBridge = Bridge(id)
    myBridge.create_ports_for_lans(LAN)





if __name__ == "__main__":
    main()
